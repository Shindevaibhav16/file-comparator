{
  "manifest_version": 3,
  "name": "File Comparator",
  "version": "1.0",
  "description": "Compare two CSV, JSON, or XML files and output their differences.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
